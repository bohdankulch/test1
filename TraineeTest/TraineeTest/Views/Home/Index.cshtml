@model IEnumerable<TraineeTest.Models.ContactManager>
@{
    ViewBag.Title = "All Manager";
}

<h1 style="text-align: center;">All Manager</h1>
<a asp-action="Create">Add Manager</a>
<table class="table">
<thead>
    <tr>
    <th>Name</th>
    <th>Date of Birth</th>
    <th>Married</th>
    <th>Phone</th>
    <th>Salary</th>
    </tr>
</thead>

<tbody>
 @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.DateOfBirth</td>
            <td>@item.Married</td>
            <td>@item.Phone</td>
            <td>@item.Salary</td>

            <td>
                <a asp-action="Details" asp-controller="Home" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Edit" asp-controller="Home" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Delete" asp-controller="Home" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
    }
    </tbody>

</table>
